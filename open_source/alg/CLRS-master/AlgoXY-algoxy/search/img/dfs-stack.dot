digraph G{
	margin="0"
	node[shape=record]
    s0[label="[s]"];
    s1[label="{[a, s] | [b, s]}"];
    sdot[shape=plaintext, label="..."];
    i[shape=plaintext];
    j[shape=plaintext];
    k[shape=plaintext];
    p[shape=plaintext];
    si[label="{[p, ... , s] | [q, ..., s] | ... }"];
    i->p[dir=back];
    j->p[dir=back];
    k->p[dir=back];
    p->si[dir=back];
    si1[label="{[i, p, ... , s] | [j, p, ..., s] | [k, p, ..., s] | [q, ..., s] | ... }"];
    {rank=same s0 s1 sdot si si1}
}
