/*unbalanced quick sort partition*/
digraph G{
	margin="0"
	node [shape=record];
	/*nil node to make the tree balanced*/
	e1[label="[]"];
	e2[label="[]"];
	e3[label="[]"];
	en[label="[]"];
        n1[label="x[1] | x[2] | ... | x[n]"];
        n2[label="x[2] | x[3] | ... | x[n]"];
        n3[label="x[3] | x[4] | ... | x[n]"];
        n[label="x[n]"];
	n1->e1;
	n2->e2;
	n3->e3;
	"..."->en[style=invis];
	n1 -> n2 -> n3 -> "..." -> n;
}
