/*unbalanced binary tree*/
digraph G{
    margin="0";
	node [shape=circle];
	/*nil node to make the tree balanced*/
	e1[label="", style=invis];
	e2[label="", style=invis];
	e3[label="", style=invis];
	en[label="", style=invis];
	1->e1[style=invis];
	2->e2[style=invis];
	3->e3[style=invis];
	"..."->en[style=invis];
	1 -> 2 -> 3 -> "..." -> n;
}
